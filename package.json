{
  "name": "iterations-server-api",
  "version": "0.0.0",
  "author": {
    "name": "Sergey Dunaevskiy",
    "email": "sergey@dunaevskiy.eu",
    "url": "https://dunaevskiy.eu/"
  },
  "main": "app.ts",
  "license": "UNLICENSED",
  "dependencies": {
    "cookie-session": "^1.3.3",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.4.19",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "pg": "^7.9.0",
    "prettier": "^1.16.4",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.15",
    "typescript": "^3.3.4000",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "scripts": {
    "start": "ts-node -P tsconfig.json -r tsconfig-paths/register app.ts",
    "// PROCESS MANAGER": "",
    "dev:start": "pm2 start --env development",
    "dev:stop": "pm2 stop Iterations-API",
    "dev:del": "pm2 del Iterations-API",
    "prod:start": "pm2 start --env production",
    "prod:stop": "pm2 stop Iterations-API",
    "prod:del": "pm2 del Iterations-API",
    "// TESTS": "",
    "test": "jest",
    "// MIGRATIONS": "",
    "migration:create": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create -n ",
    "migration:generate": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -n ",
    "migration:run": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert",
    "// SEEDS": "",
    "seed:create": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create -c seed -n ",
    "seed:run": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -c seed",
    "seed:revert": "ts-node -P tsconfig.json -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert -c seed"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "pm2": "^3.4.0",
    "ts-jest": "^24.0.0"
  }
}
